{
  "projectType": "library",
  "sourceRoot": "libs/cronus/feature-cronus/src",
  "prefix": "crn",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/cronus/feature-cronus"],
      "options": {
        "jestConfig": "libs/cronus/feature-cronus/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/cronus/feature-cronus/**/*.ts",
          "libs/cronus/feature-cronus/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:cronus", "type:feature"]
}
